@using SL.Util
@using SL.Data
@{
    RequestUtil req = new RequestUtil();
    int noteId = req.Int("noteId", false, "need noteId");

    var data = SQL.QuerySingle("select NoteID,NoteText from Note where NoteID=@p0", noteId);

    Json.Write(new { success = true, data = data }, Output);
    return;
}
